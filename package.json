{
  "name": "nuxt-og-image",
  "type": "module",
  "version": "2.0.18",
  "packageManager": "pnpm@8.15.6",
  "description": "Enlightened OG Image generation for Nuxt.",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-og-image#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-og-image.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-og-image/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && pnpm build:module && pnpm build:client",
    "build:client": "nuxi generate client",
    "build:module": "nuxt-build-module",
    "lint": "eslint \"**/*.{ts,vue,json,yml}\" --fix",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare .playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "@resvg/resvg-js": "^2.6.2",
    "@resvg/resvg-wasm": "^2.6.2",
    "@types/fs-extra": "^11.0.4",
    "birpc": "^0.2.17",
    "chalk": "^5.3.0",
    "chrome-launcher": "^1.1.1",
    "defu": "^6.1.4",
    "execa": "^7.2.0",
    "fast-glob": "^3.3.2",
    "flatted": "^3.3.1",
    "fs-extra": "^11.2.0",
    "globby": "^13.2.2",
    "image-size": "^1.1.1",
    "inline-css": "^4.0.2",
    "launch-editor": "^2.6.1",
    "nuxt-site-config": "^1.6.7",
    "nuxt-site-config-kit": "^1.6.7",
    "nypm": "^0.3.8",
    "ofetch": "^1.3.4",
    "ohash": "^1.1.3",
    "pathe": "^1.1.2",
    "playwright-core": "^1.43.0",
    "radix3": "^1.1.2",
    "satori": "0.10.13",
    "satori-html": "^0.3.2",
    "sirv": "^2.0.4",
    "std-env": "^3.7.0",
    "svg2png-wasm": "^1.4.1",
    "tinyws": "^0.1.0",
    "twemoji": "^14.0.2",
    "ufo": "^1.5.3",
    "ws": "^8.16.0",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@nuxt/devtools-edge": "0.7.4-28188711.ce53651",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/test-utils": "3.12.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/ws": "^8.5.10",
    "bumpp": "^9.4.0",
    "eslint": "8.57.0",
    "jest-image-snapshot": "^6.4.0",
    "nuxt": "^3.11.2",
    "nuxt-icon": "^0.6.10",
    "playwright": "^1.43.0",
    "sass": "^1.74.1",
    "vitest": "^0.34.6"
  },
  "resolutions": {
    "@nuxt/schema": "3.11.2"
  }
}
